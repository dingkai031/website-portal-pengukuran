<script>
  const tombolDownload = document.querySelectorAll(".tombolDownload");
  for (const download of tombolDownload) {
    download.addEventListener("click", ()=> {
      const idTable =  download.getAttribute("data-id-table");
      const idData =  download.getAttribute("data-id-table-data");
      const doc = new jspdf.jsPDF();
      console.log(idData)
      console.log(idTable)
      doc.autoTable({
        html: idTable,
        theme: "grid",
        didParseCell: function(data) {
          const titleElement = data.cell.raw;
          if (titleElement && titleElement.classList.contains('title')) {
            data.cell.styles.fontStyle = 'bold';
            data.cell.text = data.cell.text.map(e => e.toUpperCase())
          }
        },
        includeHiddenHtml : true,
        tableLineWidth : 0.4,
        tableLineColor : 0,
        styles: {
          font : "times",
          textColor : "#000",
          overflow: 'linebreak',
          cellWidth: 'wrap',
          lineColor : 0
        },
        columnStyles: {
          0: {cellWidth: 90, valign : "middle"},
          1: {cellWidth: "auto"}
        },
        
      });

      doc.autoTable({
        html: idData,
        theme: "plain",
        didParseCell: function(data) {
          const titleElement = data.cell.raw;
          if (titleElement && titleElement.classList.contains('title')) {
            data.cell.styles.fontStyle = 'bold';
            data.cell.text = data.cell.text.map(e => e.toUpperCase())
          }
        },
        includeHiddenHtml : true,
        tableLineWidth : 0.4,
        tableLineColor : 0,
        styles: {
          font : "times",
          textColor : "#000",
          overflow: 'linebreak',
          cellWidth: 'wrap',
          lineColor : 0
        },
        headStyles: {
          halign: "center",
          valign : "middle",
          lineColor : 0,
          lineWidth: 0.2
        },
        bodyStyles : {
          lineColor : 0,
          lineWidth: 0.2
        },
        columnStyles: {
          0: {cellWidth: 10, halign : "center", valign : "middle"},
          1: {cellWidth: 20, halign : "center", valign : "middle"},
          2: {cellWidth: "auto", halign : "center", valign : "middle"},
          3: {cellWidth: 40, halign : "center", valign : "middle"},
          4: {cellWidth: 40, halign : "center", valign : "middle"},
          
        },
      });
      
      doc.save(`Laporan-Inspeksi-Ketersediaan-Alat-Evakuasi-Korban-${moment().format("x")}`)
    })
  }
</script>